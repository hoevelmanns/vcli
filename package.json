{
  "name": "@hoevelmanns/vcli",
  "private": false,
  "description": "A universal and extensible CLI for Shopware, Laravel and others with vagrant support",
  "version": "0.6.6-poc",
  "author": "Marcelle HÃ¶velmanns",
  "bin": {
    "vc": "bin/run"
  },
  "bugs": {
    "url": "https://github.com/hoevelmanns/vcli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.1.0",
    "@types/node-notifier": "^6.0.1",
    "@types/utf8": "^2.1.6",
    "camelcase": "^6.0.0",
    "find-up": "^4.1.0",
    "node-notifier": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "rxjs-shell": "^2.1.2",
    "shelljs": "^0.8.4",
    "tslib": "^1",
    "typescript-interface-generator": "^0.0.6",
    "utf8": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^10",
    "prettier": "^2.0.5",
    "ts-node": "^8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/hoevelmanns/vcli/",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "cache": {
        "description": "control the Shopware cache"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoevelmanns/vcli.git"
  },
  "scripts": {
    "link": "npm link",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest",
    "posttest": "npm run lint",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix"
  },
  "types": "lib/index.d.ts"
}
